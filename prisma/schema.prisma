generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Students {
  id      Int     @id(map: "students_pkey") @default(autoincrement())
  name    String? @db.VarChar
  address String? @db.VarChar
}

model Faculty {
  id   Int    @id @default(autoincrement())
  name String
}
